{
  "name": "easyFormMemory",
  "manifest_version": 2,
  "version": "1.0.0",
  "description": "用于记忆表单，填写的表单将自动记忆",
  "browser_action": {
    "default_icon": "light.png",
    "default_title": "表单记忆小插件",
    "default_popup": ""
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["formRecord.js"],
      "run_at": "document_start"
    }
  ]
}